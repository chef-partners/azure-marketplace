{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "location": {
        "type": "string"
      },
      "storageAccountName": {
        "type": "string",
        "metadata": {
          "Description": "The name of the new storage account created to store the VMs disks"
        }
      },
      "storageAccountType": {
        "type": "string",
        "allowedValues": [
          "Standard_LRS"
        ],
        "metadata": {
          "Description": "The type of the Storage Account created"
        },
        "defaultValue": "Standard_LRS"
      },
      "tags": {
          "type": "object"
      }
    },
    "variables": {
      "apiVersions": {
        "sa": "2019-04-01"
      },

      "accountSasProperties": {
        "type": "object",
        "defaultValue": {
          "signedServices": "b",
          "signedPermission": "w",
          "signedResourceTypes": "c",
          "signedExpiry": ""
        }
      }
    },
    "resources": [
      {
        "type": "Microsoft.Storage/storageAccounts",
        "name": "[concat(parameters('storageAccountName'))]",
        "apiVersion": "[variables('apiVersions').sa]",
        "location": "[parameters('location')]",
        "tags": "[parameters('tags')]",
        "sku": {
          "name": "[parameters('storageAccountType')]"
        },
        "resources": [
          {
            "name": "default/starterkits",
            "type": "blobServices/containers",
            "apiVersion": "[variables('apiVersions').sa]",
            "dependsOn":[
              "[parameters('storageAccountName')]"
            ]
          }
        ]
      }
    ],
    "outputs": {
      "accesskey": {
        "type": "string",
        "value": "[listKeys(resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccountName')), variables('apiVersions').sa).keys[0].value]"
      }
    }
  }
  